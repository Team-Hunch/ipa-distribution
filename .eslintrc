{
    "parser": "babel-eslint",
    "rules": {
        "no-unused-expressions": 1,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "strict": 0,
        "new-cap": 0,
        "camelcase": 0,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "ecmaFeatures": {
        "jsx": false
    },
    "env": {
        "browser": false,
        "node": true,
        "es6": false
    },
    "globals": {
        "BROWSER": false
    },
    "extends": "eslint:recommended"
}